# Example .env file for System-Level LLM Agent
# Rename this file to .env and fill in your actual values.

# OpenAI API Base URL (default is https://api.openai.com/v1)
OPENAI_BASE_URL="https://api.openai.com/v1"

# OpenAI Model to use (e.g., gpt-4o, gpt-3.5-turbo)
OPENAI_MODEL="gpt-4o"

# OpenAI API Key
OPENAI_API_KEY="your_openai_api_key_here"

# WeChat Work Integration (Optional)
# Get these from your WeChat Work admin panel
WECHAT_CORPID=""
WECHAT_CORPSECRET=""
WECHAT_AGENTID=""
WECHAT_TOUSER="@all"
